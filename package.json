{
  "name": "iwwa-write-api",
  "version": "1.0.0",
  "description": "Iwwa platform write API",
  "main": "lib/server.js",
  "scripts": {
    "build": "babel src --out-dir lib",
    "clean": "rimraf lib coverage",
    "write-commit": "git rev-parse --short HEAD > COMMIT",
    "coverage": "NODE_ENV=test NODE_PATH=src babel-node $(npm bin)/isparta cover $(npm bin)/_mocha -- --recursive",
    "codecov": "cat coverage/lcov.info | codecov",
    "dev": "npm test -- --watch",
    "lint": "eslint src test",
    "prestart": "npm run write-commit && npm run build",
    "start": "node lib/server.js",
    "test": "NODE_ENV=test mocha --compilers js:babel-core/register --require babel-polyfill --recursive"
  },
  "author": "Team Wattellina <wattellina@mondora.com>",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/innowatio/iwwa-write-api.git"
  },
  "bugs": {
    "url": "https://github.com/innowatio/iwwa-write-api/issues"
  },
  "devDependencies": {
    "babel": "^6.3.13",
    "babel-cli": "^6.3.13",
    "babel-core": "^6.3.13",
    "babel-eslint": "^5.0.0-beta4",
    "babel-plugin-rewire": "^1.0.0-beta-2",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "chai": "^3.4.1",
    "coveralls": "^2.11.4",
    "eslint": "^1.10.3",
    "isparta": "^4.0.0",
    "mocha": "^2.3.4",
    "sinon": "^1.17.2",
    "sinon-chai": "^2.8.0"
  },
  "dependencies": {
    "aws-sdk": "^2.2.21",
    "bluebird": "^3.0.6",
    "body-parser": "^1.14.1",
    "bunyan": "^1.5.1",
    "bunyan-request": "^1.2.0",
    "express": "^4.13.3",
    "express-bearer-token": "^2.1.0",
    "lk-collection-express": "^1.1.0",
    "lodash.merge": "^3.3.2",
    "mongodb": "^2.0.52",
    "node-uuid": "^1.4.7"
  }
}

{
  "name": "iwwa-write-api",
  "version": "1.0.0",
  "description": "Iwwa platform write API",
  "main": "lib/server.js",
  "scripts": {
    "start-kinesis": "babel-node scripts/start-kinesis.js",
    "build": "babel src --out-dir lib",
    "clean": "rimraf lib coverage",
    "coverage": "NODE_ENV=test NODE_PATH=src babel-node $(npm bin)/isparta cover $(npm bin)/_mocha -- --recursive --require babel-polyfill",
    "codecov": "codecov",
    "lint": "eslint src test",
    "prestart": "npm run build",
    "start": "NODE_PATH=lib node lib/server.js",
    "test": "NODE_ENV=test NODE_PATH=src mocha --compilers js:babel-core/register --require babel-polyfill --recursive",
    "dev:start": "NODE_PATH=src nodemon --ignore test/ --exec node_modules/.bin/babel-node src/server.js | bunyan",
    "dev:test": "npm test -- --watch",
    "dev": "npm run dev:start & npm run dev:test"
  },
  "author": "Team Wattellina <wattellina@mondora.com>",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/innowatio/iwwa-write-api.git"
  },
  "bugs": {
    "url": "https://github.com/innowatio/iwwa-write-api/issues"
  },
  "devDependencies": {
    "babel-core": "^6.3.13",
    "babel-eslint": "^5.0.0-beta4",
    "babel-plugin-rewire": "^1.0.0-beta-2",
    "chai": "^3.4.1",
    "codecov": "^1.0.1",
    "eslint": "^1.10.3",
    "isparta": "^4.0.0",
    "kinesalite": "^1.10.2",
    "mocha": "^2.3.4",
    "nodemon": "^1.8.1",
    "sinon": "^1.17.2",
    "sinon-chai": "^2.8.0",
    "supertest": "^1.1.0",
    "supertest-as-promised": "^2.0.2"
  },
  "dependencies": {
    "aws-sdk": "^2.2.21",
    "babel-cli": "^6.3.13",
    "babel-polyfill": "^6.7.2",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "bcryptjs": "^2.3.0",
    "bluebird": "^3.0.6",
    "bunyan": "^1.5.1",
    "bunyan-cloudwatch": "^2.0.0",
    "bunyan-request": "^1.2.0",
    "convexpress": "^1.3.3",
    "express": "^4.13.3",
    "lk-collection-convexpress": "^1.0.3",
    "lk-dispatch": "^1.0.1",
    "lk-users-convexpress": "^1.0.2",
    "mongodb": "^2.0.52",
    "node-uuid": "^1.4.7",
    "ramda": "^0.20.0"
  }
}
